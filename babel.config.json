{
   "presets": [
      [
         "@babel/preset-env",
         {
            "targets": {
               "browsers": "last 2 versions"
            },
            "modules": false,
            "loose": false
         }
      ],
      "@babel/preset-react",
      "@babel/preset-typescript"
   ],
   "plugins": [
      "transform-class-properties"
   ],
   "env": {
      "test": {
         "plugins": [
            "transform-es2015-modules-commonjs"
         ]
      }
   }
}